<asserts>
    <dql-assert id="assertSelectOne">
        <dql-sub-assert sharding-rule-types="db" expected-data-file="SelectOne.xml" />
        <dql-sub-assert sharding-rule-types="tbl" expected-data-file="SelectOne.xml" />
        <dql-sub-assert sharding-rule-types="dbtbl" expected-data-file="SelectOne.xml" />
        <dql-sub-assert sharding-rule-types="masterslave" expected-data-file="SelectOne.xml" />
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTable">
        <dql-sub-assert sharding-rule-types="tbl" parameters="100000:int" expected-data-file="SelectNotEqualsWithSingleTable.xml" />
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTableForExclamationEqual">
        <dql-sub-assert sharding-rule-types="tbl" parameters="100000:int" expected-data-file="SelectNotEqualsWithSingleTable.xml" />
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTableForNotIn">
        <dql-sub-assert sharding-rule-types="tbl" parameters="100000:int, 100001:int" expected-data-file="SelectNotWithSingleTable.xml" />
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTableForNotBetween">
        <dql-sub-assert sharding-rule-types="tbl" parameters="100000:int, 100001:int" expected-data-file="SelectNotWithSingleTable.xml" />
    </dql-assert>
    <dql-assert id="assertSelectEqualsWithSameShardingColumns">
        <dql-sub-assert sharding-rule-types="tbl" parameters="1000:int, 1001:int" expected-data-file="SelectEqualsWithSameShardingColumns.xml" />
    </dql-assert>
    <dql-assert id="assertSelectInWithSameShardingColumns">
        <dql-sub-assert sharding-rule-types="tbl" parameters="1000:int, 1001:int, 1001:int, 1100:int" expected-data-file="SelectInWithSameShardingColumns.xml" />
    </dql-assert>
    <dql-assert id="assertSelectEqualsWithSingleTable">
        <dql-sub-assert sharding-rule-types="tbl" parameters="10:int, 1000:int" expected-data-file="SelectEqualsWithSingleTable_0.xml" />
        <dql-sub-assert sharding-rule-types="tbl" parameters="11:int, 1109:int" expected-data-file="SelectEqualsWithSingleTable_1.xml" />
        <dql-sub-assert sharding-rule-types="db" parameters="12:int, 1201:int" expected-data-file="SelectEqualsWithSingleTable_2.xml" />
        <dql-sub-assert sharding-rule-types="dbtbl" parameters="12:int, 1201:int" expected-data-file="SelectEqualsWithSingleTable_2.xml" />
        <dql-sub-assert sharding-rule-types="masterslave" parameters="12:int, 1201:int" expected-data-file="masterslave/SelectEqualsWithSingleTable_2.xml" />
        <dql-sub-assert sharding-rule-types="tbl" parameters="12:int, 1000:int" expected-data-file="Empty.xml" />
        <dql-sub-assert sharding-rule-types="db" parameters="12:int, 1000:int" expected-data-file="Empty.xml" />
        <dql-sub-assert sharding-rule-types="dbtbl" parameters="12:int, 1000:int" expected-data-file="Empty.xml" />
        <dql-sub-assert sharding-rule-types="masterslave" parameters="12:int, 1000:int" expected-data-file="Empty.xml" />
    </dql-assert>
    
