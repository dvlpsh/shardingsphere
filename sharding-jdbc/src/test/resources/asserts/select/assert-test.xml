<asserts>
    <dql-assert id="assertSelectOne">
        <subAssert expected-data-file="SelectOne.xml" sharding-rule-types="db" />
        <subAssert expected-data-file="SelectOne.xml" sharding-rule-types="tbl" />
        <subAssert expected-data-file="SelectOne.xml" sharding-rule-types="dbtbl" />
        <subAssert expected-data-file="SelectOne.xml" sharding-rule-types="masterslave" />
