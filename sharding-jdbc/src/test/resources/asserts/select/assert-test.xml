<asserts>
    <dql-assert id="assertSelectOne">
        <subAssert sharding-rule-types="db" expected-data-file="SelectOne.xml" />
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectOne.xml" />
        <subAssert sharding-rule-types="dbtbl" expected-data-file="SelectOne.xml" />
        <subAssert sharding-rule-types="masterslave" expected-data-file="SelectOne.xml" />
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTable">
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectNotEqualsWithSingleTable.xml">
            <parameter>
                <value value="100000" type="int"/>
            </parameter>
        </subAssert>
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTableForExclamationEqual">
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectNotEqualsWithSingleTable.xml">
            <parameter>
