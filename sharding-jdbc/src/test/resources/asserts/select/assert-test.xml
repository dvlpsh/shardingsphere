                <value value="1000" type="int"/>
                <value value="1001" type="int"/>
                <value value="1001" type="int"/>
                <value value="1100" type="int"/>
            </parameter>
        </subAssert>
    </dql-assert>
    <dql-assert id="assertSelectEqualsWithSingleTable">
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectEqualsWithSingleTable_0.xml">
            <parameter>
                <value value="10" type="int"/>
                <value value="1000" type="int"/>
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectEqualsWithSingleTable_1.xml">
            <parameter>
                <value value="11" type="int"/>
                <value value="1109" type="int"/>
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="db" expected-data-file="SelectEqualsWithSingleTable_2.xml">
            <parameter>
                <value value="12" type="int"/>
                <value value="1201" type="int"/>
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="dbtbl" expected-data-file="SelectEqualsWithSingleTable_2.xml">
            <parameter>
                <value value="12" type="int"/>
                <value value="1201" type="int"/>
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="masterslave" expected-data-file="masterslave/SelectEqualsWithSingleTable_2.xml">
            <parameter>
                <value value="12" type="int"/>
                <value value="1201" type="int"/>
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="tbl" expected-data-file="Empty.xml">
            <parameter>
                <value value="12" type="int" />
                <value value="1000" type="int" />
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="db" expected-data-file="Empty.xml">
            <parameter>
                <value value="12" type="int" />
                <value value="1000" type="int" />
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="dbtbl" expected-data-file="Empty.xml">
            <parameter>
                <value value="12" type="int" />
                <value value="1000" type="int" />
            </parameter>
        </subAssert>
        <subAssert sharding-rule-types="masterslave" expected-data-file="Empty.xml">
            <parameter>
                <value value="12" type="int" />
                <value value="1000" type="int" />
            </parameter>
        </subAssert>
    </dql-assert>
    
