        <subAssert sharding-rule-types="tbl" expected-data-file="SelectNotWithSingleTable.xml">
            <parameter>
                <value value="100000" type="int"/>
                <value value="100001" type="int"/>
            </parameter>
        </subAssert>
    </dql-assert>
    <dql-assert id="assertSelectNotEqualsWithSingleTableForNotBetween">
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectNotWithSingleTable.xml">
            <parameter>
                <value value="100000" type="int"/>
                <value value="100001" type="int"/>
            </parameter>
        </subAssert>
    </dql-assert>
    <dql-assert id="assertSelectEqualsWithSameShardingColumns">
        <subAssert sharding-rule-types="tbl" expected-data-file="SelectEqualsWithSameShardingColumns.xml">
            <parameter>
                <value value="1000" type="int"/>
                <value value="1001" type="int"/>
            </parameter>
        </subAssert>
    </dql-assert>
