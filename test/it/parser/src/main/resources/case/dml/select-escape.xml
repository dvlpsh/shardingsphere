<sql-parser-test-cases>
    <select sql-case-id="select_with_double_quotes">
        <projections start-index="7" stop-index="7">
            <shorthand-projection start-index="7" stop-index="7" />
        </projections>
        <from>
            <simple-table name="t_order_item" start-index="14" stop-index="28" />
        </from>
        <where start-index="30" stop-index="54">
            <expr>
                <binary-operation-expression start-index="36" stop-index="54">
                    <left>
                        <column name="item_id" start-index="36" stop-index="45" />
                    </left>
                    <operator>!=</operator>
                    <right>
                        <parameter-marker-expression parameter-index="0" start-index="50" stop-index="50" />
                    </right>
                </binary-operation-expression>
            </expr>
        </where>
        <order-by>
            <column-item name="item_id" order-direction="ASC" start-index="61" stop-index="70" />
        </order-by>
    </select>

