<sql-parser-test-cases>
    <select sql-case-id="select_with_block_comment">
        <projections start-index="7" stop-index="7">
            <shorthand-projection start-index="7" stop-index="7" />
        </projections>
        <from>
            <simple-table name="t_order" start-index="68" stop-index="74" />
        </from>
        <where start-index="76" stop-index="88">
            <expr>
                <binary-operation-expression start-index="82" stop-index="88">
                    <left>
                        <column name="status" start-index="82" stop-index="87" />
                    </left>
                    <operator>=</operator>
                    <right>
                        <literal-expression value="1" start-index="89" stop-index="91" />
                    </right>
                </binary-operation-expression>
            </expr>
        </where>
        <comment start-index="9" stop-index="53" text="/* this is &#x000D;&#x000A; block comment */" />
        <comment start-index="60" stop-index="104" text="/* this is another &#x000A; block comment */" />
    </select>

